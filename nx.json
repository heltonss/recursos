{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "devpr",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  },
  "projects": {
    "website": {
      "tags": ["type:app", "preset:web"]
    },
    "website-e2e": {
      "tags": [],
      "implicitDependencies": ["website"]
    },
    "apps-recorder": {
      "tags": ["type:app"]
    },
    "apps-recorder-e2e": {
      "tags": [],
      "implicitDependencies": ["apps-recorder"]
    },
    "common-web": {
      "tags": []
    },
    "example": {
      "tags": []
    }
  }
}
