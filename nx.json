{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "devpr",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "projects": {
    "common-elements": {
      "tags": [
        "type:package"
      ]
    },
    "common-web": {
      "tags": [
        "type:package"
      ]
    },
    "core-entities": {
      "tags": []
    },
    "devector": {
      "tags": [
        "type:app",
        "preset:web"
      ]
    },
    "endpoint": {
      "tags": []
    },
    "example": {
      "tags": [
        "type:app",
        "preset:web"
      ]
    },
    "presenter": {
      "tags": [
        "type:app"
      ]
    },
    "recorder": {
      "tags": [
        "type:app",
        "preset:web"
      ]
    },
    "recorder-e2e": {
      "tags": [
        "type:e2e"
      ],
      "implicitDependencies": [
        "recorder"
      ]
    },
    "server-storage": {
      "tags": []
    },
    "shared-util-collection": {
      "tags": []
    },
    "web-avatars": {
      "tags": []
    },
    "web-buttons": {
      "tags": []
    },
    "web-core": {
      "tags": []
    },
    "web-flows": {
      "tags": []
    },
    "web-forms": {
      "tags": []
    },
    "web-icons": {
      "tags": []
    },
    "website": {
      "tags": [
        "type:app",
        "preset:web"
      ]
    }
  }
}